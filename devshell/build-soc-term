### build third_party/soc_term

source "$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"/lib/vars.sh
fx-config-read

readonly SOC_TERM_PATH="${FUCHSIA_DIR}/third_party/soc_term"

echo "Building third_party/soc_term..."
make -C ${SOC_TERM_PATH}
